{
  "manifest_version": 2,
  "name": "Spell Check",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon32.png",
    
    "default_title": "Lookup a spelling!"
  }
}